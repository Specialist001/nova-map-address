{
    "name": "specialist/nova-map-address",
    "description": "A Laravel Nova field to place a marker on map to get coordinates then it reverse geocoding the coordinates to get a street address",
    "keywords": [
        "laravel",
        "nova",
        "geocoding",
        "address",
        "geo",
        "map"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "Specialist\\MapAddress\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Specialist\\MapAddress\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
